<?xml version="1.0" ?>
<robot name="jeep_car">
  <link name="base_link">
    <!-- Body -->
    <visual>
      <origin xyz="0 0 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="3.5 1.5 1.0"/>
      </geometry>
      <material name="jeep_green">
        <color rgba="0.3 0.6 0.3 1.0"/>
      </material>
    </visual>
    
    <!-- Cabin / Top -->
    <visual>
      <origin xyz="-0.5 0 1.3" rpy="0 0 0"/>
      <geometry>
        <box size="2.0 1.4 0.6"/>
      </geometry>
      <material name="dark_grey">
        <color rgba="0.2 0.2 0.2 0.9"/>
      </material>
    </visual>

    <!-- Front Left Wheel -->
    <visual>
      <origin xyz="1.2 0.9 0.4" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.4" length="0.4"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1.0"/>
      </material>
    </visual>
    <!-- Front Right Wheel -->
    <visual>
      <origin xyz="1.2 -0.9 0.4" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.4" length="0.4"/>
      </geometry>
      <material name="black"/>
    </visual>
    <!-- Rear Left Wheel -->
    <visual>
      <origin xyz="-1.2 0.9 0.4" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.4" length="0.4"/>
      </geometry>
      <material name="black"/>
    </visual>
    <!-- Rear Right Wheel -->
    <visual>
      <origin xyz="-1.2 -0.9 0.4" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.4" length="0.4"/>
      </geometry>
      <material name="black"/>
    </visual>
    
    <!-- Headlights -->
    <visual>
      <origin xyz="1.75 0.5 0.7" rpy="0 1.5707 0"/>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="0.9 0.9 0.8 1.0"/>
      </material>
    </visual>
    <visual>
      <origin xyz="1.75 -0.5 0.7" rpy="0 1.5707 0"/>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white"/>
    </visual>

  </link>
</robot>
